{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/leaderboard",
      "dest": "/pages/api/leaderboard.js"
    },
    {
      "src": "/api/game",
      "dest": "/api/game.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
    {
      "src": "/api/auth/check",
      "dest": "/api/auth/check.js"
    },
    {
      "src": "/api/leaderboard",
      "dest": "/api/leaderboard.js"
    }
    {
      "src": "/timeout-image",
      "dest": "/api/timeout-image.js"
    }
  ]
}
